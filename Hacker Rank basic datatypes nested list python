if __name__ == '__main__':
    python_list = []
    score_list = []
    for _ in range(int(input())):
        input_name = input()
        score = float(input())
        python_list.append([input_name, score])
        score_list.append(score)
print(python_list)
print(score_list)
score_list = sorted(set(score_list))
print(score_list)
second_lowest = score_list[1]
names = []
for p_list in python_list:
    if second_lowest == p_list[1]:
        names.append(p_list[0])
print(names)
names.sort()
for name in names:
    print(name)
# mahmoud batran
